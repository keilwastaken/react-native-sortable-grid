# react-native-sortable-board
# Copy this file to .env and replace values with your local settings

# --- Physics Tuning ---
# Default spring configuration for the continuous surface wiggle and lift
DEFAULT_SPRING_STIFFNESS=200
DEFAULT_SPRING_DAMPING=20
DEFAULT_LIFT_SCALE=1.1
DEFAULT_WIGGLE_ROTATION_DEG=2

# --- Feature Flags ---
# Enable visual debug overlays for boundary detection and drop zones
ENABLE_PHYSICS_DEBUG_OVERLAY=false
